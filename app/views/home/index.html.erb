<h1>Welcome to your OAuth Practice Repo!!</h1>
<p>
<%= link_to "https://github.com/login/oauth/authorize?client_id="+ @client_id + "&scope=repo" do %>
    <%= image_tag'github.png', size: "250x100" %>
<% end %>
</p>

<p>
<%= link_to "https://accounts.google.com/o/oauth2/v2/auth?client_id="+ @google_client_id + "&response_type=code" + "&scope=https://www.googleapis.com/auth/userinfo.profile" + "&redirect_uri=http://localhost:3000/auth/google_oauth2/callback"  do %>
<%= image_tag'google.png', size: "230x75" %>
<% end %>

</p>

<%= button_to "Log in with Google", '/auth/google_oauth2'  %>